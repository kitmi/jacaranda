    "description": "Gen-X Entity Models (GEM) - User",
    "private": true,
    "dependencies": {
        "@genx/app": "github:genx-tech/gx-app#v2",
        "@genx/data": "github:genx-tech/gx-data#v2",
        "@genx/jes": "*",
