{
    "name": "@kitmi/jacaranda",
    "version": "1.0.0",
    "description": "JavaScript application framework",
    "main": "cjs/index.js",
    "module": "src/index.js",
    "react-native": "src/index.js",
    "exports": {
        ".": {
            "import": "./src/index.js",
            "require": "./cjs/index.js"
        },
        "./*": {
            "import": "./src/*.js",
            "require": "./cjs/*.js"
        },
        "./**/*": {
            "import": "./src/**/*.js",
            "require": "./cjs/**/*.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "author": "Rockie Guo <rockie@kitmi.com.au>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kitmi/jacaranda.git"
    },
    "license": "MIT",
    "scripts": {
        "test": "mocha --recursive test/**/*.spec.js",
        "doc": "jsdoc --configure .jsdoc.json --verbose",
        "cover": "nyc --reporter=html --reporter=text mocha --recursive test/**/*.spec.js && open ./coverage/index.html",
        "prettier": "prettier --write .",
        "lint": "eslint \"**/*.js\"",
        "lint-fix": "eslint \"**/*.js\" --fix",
        "clean": "shx rm -rf cjs coverage .nyc_output",
        "build": "bun run clean && swc src/**/*.js -d ./cjs"
    },
    "keywords": [
        "kitmi",
        "jacaranda",
        "app",
        "framework"
    ],
    "nyc": {
        "exclude": [
            ".*",
            "cjs",
            "test",
            "**/*.spec.js"
        ]
    },
    "dependencies": {
        "@kitmi/adapters": "workspace:*",
        "@kitmi/algo": "workspace:*",
        "@kitmi/config": "workspace:*",
        "@kitmi/jsonv": "workspace:*",
        "@kitmi/jsonx": "workspace:*",
        "@kitmi/sys": "workspace:*",
        "@kitmi/types": "workspace:*",
        "@kitmi/utils": "workspace:*",
        "glob": "^10.3.10"
    },
    "devDependencies": {
        "capture-console": "^1.0.1",
        "jsonwebtoken": "^9.0.0",
        "koa": "^2.14.2",
        "koa-body": "^6.0.1",
        "koa-compress": "^5.1.1",
        "koa-etag": "^4.0.0",
        "koa-mount": "^4.0.0",
        "koa-static": "^5.0.0",
        "nanoid": "^3.3.7"
    },
    "prettier": {
        "quoteProps": "consistent",
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "es5",
        "tabWidth": 4,
        "useTabs": false
    }
}